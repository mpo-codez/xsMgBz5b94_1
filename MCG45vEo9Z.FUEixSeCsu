function __getAll__Parts__()
	local allParts = {}
	for _, object in ipairs(workspace:GetDescendants()) do
		if object:IsA("BasePart") then
			table.insert(allParts, object)
		end
	end
	return allParts
end

function __list_folder(folder: Instance, filter: string)
	local result = {}
	for i, obj in pairs(folder:GetDescendants()) do
		if filter then
			if obj:IsA(filter) then
				table.insert(result, {obj = obj, name = obj.name, location = obj:GetFullName(), type = obj.className})
			end
		else
			table.insert(result, {obj = obj, name = obj.name, location = obj:GetFullName(), type = obj.className})
		end
	end
	return result
end

function __list_all_in_folder(folder: Instance)
	local content = __list_folder(folder)
	local result = {}
	for k, info in pairs(content) do
		if info.className == "Folder" then
			table.insert(result, __list_all_in_folder(info.obj))
		else
			table.insert(result, info)
		end
	end
	return result
end

