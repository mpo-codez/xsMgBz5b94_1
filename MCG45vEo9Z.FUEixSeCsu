function __set_opt_to_obj_in_folder(folder: Instance, filter: string, opt: string, new: any)
	local content = __list_folder(folder, filter)
	for k, part in content do
		task.wait()
		part.obj[opt] = new
	end
end

function __new_instance_in_all_on_folder(folder: Instance, filter: string, newType: string, opts)
	opts = opts or {}
	local content = __list_folder(folder, filter)
	for key, inst in pairs(content) do
		task.wait()
		local obj = Instance.new(newType)
		obj.Parent = inst.obj
		for k, v in pairs(opts) do
			obj[k] = v
		end
	end
end
